{
  "manifest_version": 2,
  "name": "noCaptchaAi: CAPTCHA Solver",
  "version": "1.4",
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "author": "noCaptchaAi.com",
  "description": "reCaptcha v2, hCaptcha, OCR/ImagetoText Recognition based on NoCaptchaAi.com ",
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "webRequest",
    "webNavigation",
    "declarativeNetRequest",
    "contextMenus",
    "activeTab",
    "unlimitedStorage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/16n.png",
      "48": "icons/48n.png"
    },
    "default_title": "noCaptchaAi",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "firefox@noCaptchaAi.com",
      "strict_min_version": "82.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/elementPicker.js"
      ],
      "all_frames": true,
      "match_about_blank": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/hCaptcha.js"
      ],
      "all_frames": true,
      "match_about_blank": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/bls.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "match_about_blank": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/ocr.js"
      ],
      "all_frames": true,
      "match_about_blank": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/iframes.js"
      ],
      "all_frames": true,
      "match_about_blank": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/recaptcha_audio.js"
      ],
      "all_frames": true,
      "match_about_blank": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/recaptcha.js"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    },
    {
      "matches": [
        "https://newconfig.nocaptchaai.com/?APIKEY=*"
      ],
      "js": [
        "src/config.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16n.png",
    "32": "icons/32n.png",
    "48": "icons/48n.png",
    "128": "icons/128n.png"
  }
}