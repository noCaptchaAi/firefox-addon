  async function getPlanType() {
    try {
      const response = await fetch("https://manage.nocaptchaai.com/api/user/get_endpoint", {
        headers: {
          apikey: settings.APIKEY,
        },
      });
  
      if (!response.ok) {
        throw new Error("API request failed");
      }
  
      return await response.json();
    } catch (error) {
      throw new Error("API request failed");
    }
  }
  
  chrome.runtime.onMessage.addListener(async (request, sender, sendResponse) => {
    if (request.action === "getPlanType") {
      try {
        const planType = await getPlanType();
        sendResponse({ success: true, value: planType });
      } catch (error) {
        sendResponse({ success: false, error: error.message });
      }
      return true;
    }
  });

  

